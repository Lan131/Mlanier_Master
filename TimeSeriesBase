data=ts(c(884740,
971479,
1786302,
1150300,
1107227,
1336403,
940836,
989800,
1344317,
1470808,
1522639,
1339789,
615107,
694785,
2039621,
707131,
869633
),start=c(2014, 2), end=c(2015, 6), frequency=12)

fit=HoltWinters(data ,alpha = NULL, beta = NULL, gamma = NULL,
            seasonal = c("additive", "multiplicative")
            )
summary(fit)
plot(fit)
plot(fitted(fit))
lines(fitted(fit)[,1], col = 3)
predict(fit, n.ahead = 12, prediction.interval = TRUE,
       level = 0.95)
plot(data)
